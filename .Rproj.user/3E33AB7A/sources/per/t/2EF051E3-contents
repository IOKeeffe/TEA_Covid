#opens file browser
car_data_1 = read.csv(choose.files())
#just imports
car_data_2 = read.csv(mtcars_loc)

#Exporting (need package to write xlsx)
write.xlsx(car_data_2, file="Matrix_random.xlsx")
write.csv(car_data_2, file="Matrix_random.csv", row.names=FALSE)
write.table(car_data_2, file="Matrix_random", sep=",")

